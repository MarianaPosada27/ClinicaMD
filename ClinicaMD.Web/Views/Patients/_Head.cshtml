@model ClinicaMD.Web.Models.ClinicHistory
<div class="row">
               <div class="column">
                   <div class="form-group">
                       <label asp-for="Patient.FullName" class="control-label"></label>
                       <input asp-for="Patient.FullName" class="form-control" />
                       <span asp-validation-for="Patient.FullName" class="text-danger"></span>
                   </div>
                   <div class="form-group">
                       <label asp-for="Patient.Document" class="control-label"></label>
                       <input asp-for="Patient.Document" class="form-control" />
                       <span asp-validation-for="Patient.Document" class="text-danger"></span>
                   </div>
                   <div class="form-group">
                       <label asp-for="Patient.PhoneNumber" class="control-label"></label>
                       <input asp-for="Patient.PhoneNumber" class="form-control" />
                       <span asp-validation-for="Patient.PhoneNumber" class="text-danger"></span>
                   </div>
                   <div class="form-group">
                       <label asp-for="Patient.Address" class="control-label"></label>
                       <input asp-for="Patient.Address" class="form-control" />
                       <span asp-validation-for="Patient.Address" class="text-danger"></span>
                   </div>
              </div>
              <div class="column">

              </div>
           </div>

 <div class="row">
    <div class="col-md-12">
        <div class="panel panel-default">
            <div class="panel-heading">
                 <h3 class="panel-title">Tipos de Procedimientos</h3>
            </div>
            <div class="panel-body">
                <table class="table table-hover table-responsive table-striped" id="MyTable">
                    <thead>
                        <tr>
                            <th>
                                @Html.DisplayNameFor(model => model.Patient.Procedures.FirstOrDefault().Description)
                            </th>
                            <th>
                                @Html.DisplayNameFor(model => model.Patient.Procedures.FirstOrDefault().ProcedureType.Description)
                            </th>
                            <th>
                                @Html.DisplayNameFor(model => model.Patient.Procedures.FirstOrDefault().Doctor.FullName)
                            </th>
                            <th></th>
                        </tr>
                    </thead>
                    <tbody>
                @foreach (var item in Model.Patient.Procedures)
                    {
                      <tr>
                        <td>
                        @Html.DisplayFor(modelItem => item.Description)
                        </td>
                         <td>
                        @Html.DisplayFor(modelItem => item.ProcedureType.Description)
                        </td>
                        <td>
                        @Html.DisplayFor(modelItem => item.Doctor.FullName)
                        </td>
                      </tr>
                       }
                    </tbody>
                </table>
            </div>
        </div>
    </div>
</div>
